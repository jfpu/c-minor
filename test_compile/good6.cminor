// Test case 6
// Arithmetic operators on local variables

main: function integer () = {
  a1: integer = 15;
  a2: integer = 7;

  print a1, '\n';
  print a2, '\n';

  print a1 + a2, '\n';
  print a2 + a1, '\n';
  print a1 - a2, '\n';
  print a2 - a1, '\n';
  print a1 * a2, '\n';
  print a2 * a1, '\n';
  print a1 / a2, '\n';
  print a2 / a1, '\n';
  print a1 % a2, '\n';
  print a2 % a1, '\n';

  print -a1 + -a2;

  print a1++, ' ', a1, '\n';
  print a2++, ' ', a2, '\n';
  print a1 = a2, ' ', a1, ' ', a2, '\n';

  return 0;
}
